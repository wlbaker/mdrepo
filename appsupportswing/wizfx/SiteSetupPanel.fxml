                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Directory: " GridPane.columnIndex="1" GridPane.rowIndex="1" />
